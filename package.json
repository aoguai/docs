{
  "name": "gkd-monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --cache --write --ignore-unknown .",
    "dev": "pnpm build:api && pnpm -F @gkd-kit/docs docs:dev",
    "build:api": "pnpm -F @gkd-kit/api predocs:build",
    "build": "pnpm build:api && pnpm -F @gkd-kit/docs docs:build",
    "preview": "pnpm -F @gkd-kit/docs docs:preview"
  },
  "dependencies": {
    "@gkd-kit/selector": "0.5.22",
    "@types/file-saver": "2.0.7",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.3",
    "@vueuse/core": "13.1.0",
    "acorn-walk": "8.3.4",
    "cross-env": "7.0.3",
    "file-saver": "2.0.5",
    "jsdom": "26.0.0",
    "magic-string": "0.30.17",
    "markdown-it-mathjax3": "4.3.2",
    "naive-ui": "2.41.0",
    "prettier": "3.5.3",
    "tsx": "4.19.4",
    "typedoc": "0.28.3",
    "typedoc-plugin-markdown": "4.6.3",
    "typedoc-vitepress-theme": "1.1.2",
    "typescript": "5.8.3",
    "unocss": "66.0.0",
    "unplugin-data": "0.1.1",
    "vitepress": "1.6.3"
  },
  "volta": {
    "node": "22.15.0",
    "pnpm": "10.10.0"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights",
        "vite"
      ]
    }
  }
}
